/* pages/itemlist/itemlist.wxss */
.header {
  width: 100%;
  /* height: 120rpx; */
  padding: 26rpx 0;
  background-color: #00A4FF;
}

.page-item {
  width: 90%;
  margin: 0 auto;
}

.main {
  width: 90%;
  margin: 0 auto;
}

.item-list item:nth-child(2n) {
  margin-left: 18rpx;
}

.item-wrapper {
  display: inline-block;
  margin: 0 auto;
  margin-top: 20rpx;
  width: calc(50% - 10rpx);
  background-color: #fff;
  border-radius: 20rpx;
}
